//m[i]ï¼š the min # of  bags that can keep i swags
//base m[1] = 1
//T: O(n^1.5)
//S: O(n)

private int minBox(int input) {
	if(i <= 1) {
		return i;
	}
	int[] m = new int[input+1];
	m[1] = 0;
	for(int i = 2; i < input; i++) {
		for(int j = 1; j*j <=i ; j++) {
		m[i] = i;
				m[i] = Math.min(m[i], m[i-j*j}+ 1);
		}
	}
	return m[i];
}
