public class Solution {
  public long power(int a, int b) {
    // Write your solution here
    //T: O(b), S: O(log n)
    if(b == 0) {
      return 1;
    }
    // b == 1 ==>已经在后面的case中覆盖了
    // 这里要先算出来，否则后面会call 两次，time out
    long res = power(a,b/2);
    if(b % 2 == 0) {
      return res*res;
    }
    return res*res*a;
  }
}
//如果b 是负数，返回类型是double
