public class Solution {
  public int[] solve(int[] array) {
    // Write your solution here
    if(array.length == 1) {
      return array;
    }
    for (int i = 0 ; i < array.length - 1 ; i++) {
      for (int j = i ; j < array.length ; j++) {
        if (array[i] >= array[j] ){
          int temp = array[i];
          array[i] = array[j];
          array[j] = temp;
        }
      }
    }
    return array;
  }
}
